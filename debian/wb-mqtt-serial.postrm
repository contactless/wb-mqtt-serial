#!/bin/bash

CONFFILE=/etc/wb-mqtt-serial.conf

if [ "$1" = "purge" ]; then
    if [ -x /usr/bin/ucf ]; then
        ucf  --purge $CONFFILE
        rm -f $CONFFILE
        rm -f $CONFFILE.simple
        rm -f $CONFFILE.default
    fi
fi

rm -f /usr/share/wb-mqtt-confed/schemas/wb-mqtt-serial.schema.json

